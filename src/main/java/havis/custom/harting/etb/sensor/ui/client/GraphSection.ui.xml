<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:l="urn:import:havis.net.ui.shared.client.list"
	xmlns:t="urn:import:havis.custom.harting.etb.sensor.ui.client.subscriber.table"
	xmlns:s="urn:import:havis.net.ui.shared.client">

	<ui:with type="havis.net.ui.shared.resourcebundle.ResourceBundle"
		field="res" />
	<ui:with
		type="havis.custom.harting.etb.sensor.ui.resourcebundle.ConstantsResource"
		field="langLocal" />
	<ui:with
		type="havis.custom.harting.etb.sensor.ui.resourcebundle.AppResources"
		field="localRes"></ui:with>
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<s:SectionHeader>
			<g:FlowPanel width="100%" height="100%" styleName="{localRes.css.row}">
				<g:Button ui:field="delete" styleName="{localRes.css.clearButton}"></g:Button>
				<g:Button ui:field="export" styleName="{localRes.css.exportButton}"></g:Button>
			</g:FlowPanel>
		</s:SectionHeader>
		<g:FlowPanel width="100%" height="100%" ui:field="graph"
			styleName="{localRes.css.graph}">
		</g:FlowPanel>
		<g:FlowPanel width="100%" height="500px" ui:field="graphPlaceholder"
			styleName="{localRes.css.graph}">
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 